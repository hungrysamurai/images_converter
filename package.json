{
  "name": "image_converter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0",
    "npm": "please-use-pnpm"
  },
  "scripts": {
    "dev": "vite --host --base=/projects/images_converter",
    "build": "vite build --base=./",
    "build:portfolio": "vite build --outDir ../../server/dist/public/projects/images_converter --base=/projects/images_converter",
    "build:convert-it.ru": "cross-env VITE_TARGET_DOMAIN=convert-it.ru vite build --outDir convert-it.ru --base=./",
    "lint": "eslint src --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format:fix": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "analyze": "analyze --analyzerPort 5175",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "node -e \"if(process.env.npm_execpath.indexOf('pnpm') === -1) { console.error('\\n\\x1b[31mâœ– Please use pnpm to install dependencies\\x1b[0m\\n'); process.exit(1); }\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.4.7",
    "gif.js": "^0.2.0",
    "gifuct-js": "^2.1.2",
    "jszip": "^3.10.1",
    "jszip-utils": "^0.1.0",
    "libheif-js": "^1.18.2",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^3.9.179",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.15",
    "utif": "^3.1.0"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/utif": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "only-allow": "^1.2.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.1.1",
    "vite-bundle-analyzer": "^0.20.1"
  },
  "overrides": {
    "eslint": "9.27.0"
  }
}
